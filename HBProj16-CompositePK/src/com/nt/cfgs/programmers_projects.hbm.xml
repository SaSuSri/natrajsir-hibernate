 <!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping> 
	<class name="com.nt.domain.Programmers_Projects"  table="Programmers_Projects">
	   <composite-id name="id"  class="com.nt.cid.Programmer_ProjectId">
	       <key-property name="prgmr_id" column="prgmr_id"/>
	       <key-property name="proj_id" column="proj_id"/>
   	   </composite-id>
		<property name="proj_name" />
		<property name="prgmr_name"/>
	</class>
</hibernate-mapping>